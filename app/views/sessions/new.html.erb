<%= flash[:login] %>

<div id="login">
	<%= form_for :session, url: sessions_path do |f| %>
		<%= f.text_field :email, placeholder: "Email" %><br>
		<%= f.password_field :password, placeholder: "Password" %><br>
		<%= f.submit "Log In" %>
	<% end %>

	<%= link_to "Sign Up", new_person_path %>
</div>
